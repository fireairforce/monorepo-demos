{
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "web#dev": {
      "dependsOn": [
        "eslint-config-custom#build",
        "ui#build",
        "tsconfig#build"
      ]
    }
  }
}
